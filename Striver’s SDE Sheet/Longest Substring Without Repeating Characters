#include <bits/stdc++.h> 
int uniqueSubstrings(string s)
{
   unordered_map<char,int>mp;
        int l =-1,ans = 0,n = s.size();
        for(int i=0;i<n;i++){
           if(mp.find(s[i])!=mp.end()){
                 l = max(l,mp[s[i]]);
           }
            ans = max(ans,i-l); 
           mp[s[i]] = i;
        }
        return ans;
}
